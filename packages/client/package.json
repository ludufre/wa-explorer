    "test": "ng test --watch=false",
    "test:watch": "ng test",
    "e2e": "pnpm run build:prod && playwright test -c e2e/playwright.config.ts e2e/",
    "e2e:show-trace": "playwright show-trace e2e/tracing/trace.zip",
